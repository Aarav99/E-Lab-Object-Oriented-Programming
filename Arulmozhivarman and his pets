#include<iostream>
using namespace std;

class catanddog

{
 public:
  int a[10],b[10],c[10],n,i;
  void count()
  {
   cin>>n;
  for(i=0;i<n;i++)
  {
    cin>>a[i]>>b[i]>>c[i];
  }
  for(i=0;i<n;i++)
  {
    if(c[i]==4||c[i]==8)
    {
	cout<<"yes";
    }
    else
    {
      cout<<"no";
    }
    cout<<endl;
  }
  }

};
int main() 
{
  catanddog obj;
  obj.count();
  
  
     
	return 0;
}
